#include <iostream> 
#include <string> 
using namespace std;

/*Variant 10 - Удалить все пробелы из строки. */

int main()
{
    
string str;
cout << "Input your string:" << endl;
getline(cin, str);

for(int i = 0; i < str.length(); i++) 
  {
   if(str[i] == ' ') 
  {
   str.erase(i,1);
   i--;
  }
cout << "Result: " << str << endl;
  }
  
}
